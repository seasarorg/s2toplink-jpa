CREATE TABLE CUSTOMER(ID NUMERIC(4) PRIMARY KEY,NAME VARCHAR(10), ADDRESS VARCHAR(60),PHONE VARCHAR(20), AGE NUMERIC(3), BIRTHDAY DATE,SEX NUMERIC(2), VERSION NUMERIC(4))
CREATE TABLE PRODUCT(ID NUMERIC(4) PRIMARY KEY,NAME VARCHAR(10), CUSTOMER_ID NUMERIC(4), VERSION NUMERIC(4), FOREIGN KEY (CUSTOMER_ID) REFERENCES CUSTOMER (ID))

